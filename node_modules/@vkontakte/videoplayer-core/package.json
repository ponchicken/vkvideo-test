{
  "name": "@vkontakte/videoplayer-core",
  "version": "2.0.71",
  "author": "vk.com",
  "description": "Videoplayer core library based on the vk.com platform",
  "homepage": "https://vk.com",
  "license": "GPL-3.0",
  "main": "esnext.cjs.js",
  "module": "esnext.esm.js",
